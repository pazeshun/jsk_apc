<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="right_vaccum_gripper">
  <xacro:include filename="$(find jsk_2016_01_baxter_apc)/robots/common_constants.xacro"/>
  <!--Link bodies-->
  <xacro:property name="rect_tube_l" value="0.285" />
  <xacro:property name="rect_tube_h" value="0.06" />
  <xacro:property name="rect_tube_w" value="0.0952" />
  <xacro:property name="cyl_tube_x" value="0.005" />
  <xacro:property name="cyl_tube_z" value="0.038" />
  <xacro:property name="cyl_tube_r" value="0.018" />
  <xacro:property name="cyl_tube_l" value="0.107" />
  <xacro:property name="connection_z" value="-0.00725" />
  <xacro:property name="connection_r" value="0.03" />
  <xacro:property name="connection_l" value="0.0905" />
  <xacro:property name="pad_base_z" value="0.025" />
  <xacro:property name="pad_base_w" value="0.09" />
  <xacro:property name="pad_base_h" value="0.038" />
  <xacro:property name="pad_base_d" value="0.06" />
  <xacro:property name="pad_r" value="0.026" />
  <xacro:property name="pad_l" value="0.03" />
  <xacro:property name="pad_x" value="-0.0005" />
  <xacro:property name="pad_y" value="0.0085" />
  <xacro:property name="pad_z" value="0.07" />
  <!--Joints-->
  <xacro:property name="right_hand_offset_x" value="-0.01" />
  <xacro:property name="right_hand_offset_z" value="0" />
  <xacro:property name="right_gripper_joint_offset_x" value="0.005" />
  <xacro:property name="right_gripper_joint_offset_y" value="-0.0085" />
  <xacro:property name="right_gripper_joint_offset_z" value="0.264" />

  <link name="link0">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link0.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey">
        <color rgba=".5 .5 .5 1"/>
      </material>
    </visual>
  </link>
  <link name="l_link1">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="-0.00625 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="r_link1">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="-0.00625 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link1.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="l_link2">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="r_link2">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link2.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="l_link3">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0.0125"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="r_link3">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0.0125"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/link3.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="tube">
    <visual>
      <origin rpy="${M_PI/2} -${M_PI/2} 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/tube.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="lightgrey"/>
    </visual>
  </link>
  <link name="nozzle">
    <visual>
      <origin rpy="0 ${M_PI/2} 0" xyz="0 0 0.030"/>
      <geometry>
        <mesh filename="package://jsk_2016_01_baxter_apc/meshes/visual/ideal_hand_stl/nozzle.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="green">
        <color rgba="0 0.7 0 1"/>
      </material>
    </visual>
  </link>
  <link name="ver_cyl">
    <visual>
      <origin rpy="0 ${M_PI/2} 0" xyz="-0.040 0 0.040"/>
      <geometry>
        <cylinder radius="0.040" length="0.100"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>
  <link name="box">
    <visual>
      <origin rpy="0 0 0" xyz="0.020 0 0.020"/>
      <geometry>
        <box size="0.100 0.080 0.040"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>
  <link name="hor_cyl">
    <visual>
      <origin rpy="${M_PI/2} 0 0" xyz="-0.010 0 0.050"/>
      <geometry>
        <cylinder radius="0.030" length="0.100"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>
  <link name="sphere">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.035"/>
      <geometry>
        <sphere radius="0.035"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
  </link>

  <joint name="link0_joint_fixed" type="fixed">
    <origin rpy="0 0 0" xyz="${right_hand_offset_x} 0 ${right_hand_offset_z}" />
    <parent link="right_hand"/>
    <child link="link0"/>
  </joint>
  <joint name="l_link1_joint" type="revolute">
    <origin rpy="0 0 0" xyz="-0.025 0.0175 0.18" />
    <parent link="link0"/>
    <child link="l_link1"/>
    <axis xyz="0 0 -1"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="r_link1_joint" type="revolute">
    <origin rpy="0 0 0" xyz="-0.025 -0.0175 0.18" />
    <parent link="link0"/>
    <child link="r_link1"/>
    <axis xyz="0 0 -1"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
    <mimic joint="l_link1_joint" multiplier="-1" offset="0"/>
  </joint>
  <joint name="l_link2_joint" type="revolute">
    <origin rpy="0 0 0" xyz="-0.0125 0 0.0145" />
    <parent link="l_link1"/>
    <child link="l_link2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="r_link2_joint" type="revolute">
    <origin rpy="0 0 0" xyz="-0.0125 0 0.0145" />
    <parent link="r_link1"/>
    <child link="r_link2"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="l_link3_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.035" />
    <parent link="l_link2"/>
    <child link="l_link3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="r_link3_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.035" />
    <parent link="r_link2"/>
    <child link="r_link3"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="tube_joint" type="prismatic">
    <origin rpy="0 0 0" xyz="0.03 0 0" />
    <parent link="link0"/>
    <child link="tube"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0.18" effort="0" velocity="4.5"/>
  </joint>
  <joint name="nozzle_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.19" />
    <parent link="tube"/>
    <child link="nozzle"/>
    <axis xyz="0 1 0"/>
    <limit lower="-1.58" upper="1.58" effort="0" velocity="4.5"/>
  </joint>
  <joint name="fixed_to_ver_cyl" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.030" />
    <parent link="nozzle"/>
    <child link="ver_cyl"/>
  </joint>
  <joint name="fixed_to_box" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.030" />
    <parent link="nozzle"/>
    <child link="box"/>
  </joint>
  <joint name="fixed_to_hor_cyl" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.180" />
    <parent link="link0"/>
    <child link="hor_cyl"/>
  </joint>
  <joint name="fixed_to_sphere" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.030" />
    <parent link="nozzle"/>
    <child link="sphere"/>
  </joint>
</robot>
